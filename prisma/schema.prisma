generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id(map: "users_pk") @default(autoincrement())
  name     String @db.VarChar(15)
  email    String @unique @db.VarChar(40)
  password String
}
